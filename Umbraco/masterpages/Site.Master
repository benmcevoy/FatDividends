                    </section>
                </div>

                <div id="widgets">
                    <section>
                        <asp:ContentPlaceHolder ID="Widgets" runat="server">
                        </asp:ContentPlaceHolder>
                    </section>
                </div>
            </div>

            <div id="footer">
                <footer>
                    <div id="copyright">
                        &copy;Fat Dividends | <a href="/blog/">Blog</a> | <a href="/contact/">Contact</a>
                    </div>
                    <%--<div class="advert">
                        <script type="text/javascript"><!--
